<h3>US Voters represented by  $option</h3>
<table class="table">
    <thead>
    <tr>
        #foreach ($item in $q1.get(0).entrySet())
            <td>
                $item.getKey()
            </td>
        #end
    </tr>
    </thead>
    <tbody>
        #foreach($map in $q1)
        <tr>
            #foreach($thing in $map.entrySet())
                <td>$thing.getValue()</td>
            #end
        </tr>
        #end
    </tbody>
</table>